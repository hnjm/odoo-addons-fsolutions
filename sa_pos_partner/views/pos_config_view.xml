<?xml version="1.0"?>
<odoo>
    <data>
        <record id="pos_config_view_form_pos_partner" model="ir.ui.view">
            <field name="name">pos.config.form.pos.partner</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">
                <div id="auto_printing" position="after">
                    <div class="col-xs-12 col-md-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="is_print_partner"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="is_print_partner"/>
                        </div>
                    </div>
                </div>
            </field>
        </record>
    </data>
</odoo>


